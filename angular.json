{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "cache": {
      "enabled": false
    }
  },
  "projects": {
     "franceMunicipalitiesAutocomplete": {
      "projectType": "library",
      "root": "projects/france-municipalities-autocomplete",
      "sourceRoot": "projects/france-municipalities-autocomplete/src",
      "prefix": "dev",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/france-municipalities-autocomplete/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/france-municipalities-autocomplete/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/france-municipalities-autocomplete/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/france-municipalities-autocomplete/src/test.ts",
            "tsConfig": "projects/france-municipalities-autocomplete/tsconfig.spec.json",
            "karmaConfig": "projects/france-municipalities-autocomplete/karma.conf.js"
          }
        }
      }
    }
  }
}
